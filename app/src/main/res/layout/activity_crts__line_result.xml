<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".CRTS_LineResult">

    <ImageView
        android:layout_width="800dp"
        android:layout_height="77dp"
        android:background="@drawable/result_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="39dp"
        android:layout_marginTop="26dp"
        android:text="CRTS Task"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/line_result_quit_button"
        android:layout_width="90dp"
        android:layout_height="38dp"
        android:layout_marginStart="675dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/quit_button"
        android:text="종료"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="102dp"
        android:text="14. Line (선 긋기)"
        android:textStyle="bold"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="720dp"
        android:layout_height="344dp"
        android:id="@+id/present_line"
        android:layout_marginStart="40dp"
        android:layout_marginTop="157dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="700dp"
        android:layout_height="108.33dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="533dp"
        android:src="@drawable/spiral_result3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/hz_score"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="587dp"
        android:gravity="center"
        android:text="4"
        android:textColor="#37BFA7"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/magnitude_score"
        android:layout_width="58dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="587dp"
        android:gravity="center"
        android:text="0.5"
        android:textColor="#37BFA7"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/distance_score"
        android:layout_width="47dp"
        android:layout_height="40dp"
        android:layout_marginStart="342dp"
        android:layout_marginTop="587dp"
        android:gravity="center"
        android:text="1"
        android:textColor="#37BFA7"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/time_score"
        android:layout_width="49dp"
        android:layout_height="40dp"
        android:layout_marginStart="483dp"
        android:layout_marginTop="587dp"
        android:gravity="center"
        android:text="3"
        android:textColor="#37BFA7"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/speed_score"
        android:layout_width="56dp"
        android:layout_height="40dp"
        android:layout_marginStart="607dp"
        android:layout_marginTop="587dp"
        android:gravity="center"
        android:text="12"
        android:textColor="#37BFA7"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <RadioGroup
        android:id="@+id/crg12_1"
        android:layout_width="720dp"
        android:layout_height="288dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="650dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/crts14_1_0"
            android:layout_width="30dp"
            android:layout_marginLeft="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/radio_button"
            android:button="@android:color/transparent" />

        <RadioButton
            android:id="@+id/crts14_1_1"
            android:layout_width="30dp"
            android:layout_marginLeft="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/radio_button"
            android:button="@android:color/transparent" />

        <RadioButton
            android:id="@+id/crts14_1_2"
            android:layout_width="30dp"
            android:layout_marginLeft="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/radio_button"
            android:button="@android:color/transparent" />

        <RadioButton
            android:id="@+id/crts14_1_3"
            android:layout_width="30dp"
            android:layout_marginLeft="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/radio_button"
            android:button="@android:color/transparent" />

        <RadioButton
            android:id="@+id/crts14_1_4"
            android:layout_width="30dp"
            android:layout_marginLeft="23dp"
            android:layout_height="30dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/radio_button"
            android:button="@android:color/transparent" />
    </RadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="113dp"
        android:layout_marginTop="685dp"
        android:text="0. 정상"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="113dp"
        android:layout_marginTop="733dp"
        android:text="1. 미약한 정도로 떨리며 간혹 주변 선에 닿기도 한다."
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="113dp"
        android:layout_marginTop="781dp"
        android:text="2. 어느 정도 떨리며 자주 주변 선에 닿는다."
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="113dp"
        android:layout_marginTop="829dp"
        android:text="3. 매우 어렵게 그림을 그리고 심하게 실수를 범한다."
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="113dp"
        android:layout_marginTop="877dp"
        android:text="4. 그림 그리기를 끝마칠 수 없다."
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/line_next"
        android:layout_width="144dp"
        android:layout_height="47dp"
        android:layout_marginStart="620dp"
        android:layout_marginTop="1141dp"
        android:background="@drawable/next_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/writing_pre"
        android:layout_width="144dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="1141dp"
        android:background="@drawable/previous_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>